<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
        <id>GreyCorbel.Logging.AppInsights</id>
        <version>$version$</version>
        <title>Skoda Auto Telemetry Standardization package</title>
        <authors>Jiri Formacek, GreyCorbel Solutions</authors>
        <owners>GreyCorbel Solutions</owners>
        <license type="expression">MIT</license>
        <projectUrl>https://tfs.skoda-auto.com/tfs/B2C_Operation/AzureGovernance/_git/Telemetry</projectUrl>
        <icon>images\icon.png</icon>
        <repository type="git" url="https://tfs.skoda-auto.com/tfs/B2C_Operation/AzureGovernance/_git/Telemetry" commit="$commit$"/>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Microsoft Application Insights TelemetryClient wrapper that enforces Skoda Auto logging standards. Provided as reference implementation for .NET Framework/Core based projects.</description>
        <releaseNotes>Initial release</releaseNotes>
        <copyright></copyright>
        <tags>AppInsights Telemetry</tags>
        <references>
            <group targetFramework=".NETStandard2.1">
            <reference file="Skoda.Logging.AppInsights.dll" />
            <reference file="Skoda.Logging.AppInsights.xml" />
            </group>
            <group targetFramework=".NETStandard2.0">
            <reference file="Skoda.Logging.AppInsights.dll" />
            <reference file="Skoda.Logging.AppInsights.xml" />
            </group>
            <group targetFramework="net45">
            <reference file="Skoda.Logging.AppInsights.dll" />
            <reference file="Skoda.Logging.AppInsights.xml" />
            </group>
        </references>
        <dependencies>
	        <group targetFramework=".NETFramework4.5">
		        <dependency id="Microsoft.ApplicationInsights" version="2.14.0" exclude="Build,Analyzers" />
	        </group>
	        <group targetFramework=".NETStandard2.1">
		        <dependency id="Microsoft.ApplicationInsights" version="2.14.0" exclude="Build,Analyzers" />
	        </group>
	        <group targetFramework=".NETStandard2.0">
		        <dependency id="Microsoft.ApplicationInsights" version="2.14.0" exclude="Build,Analyzers" />
	        </group>
        </dependencies>
    </metadata>
    <files>
    <file src=".\images\icon.png" target="images\" />
    <file src="bin\release\netstandard2.0\Skoda.Logging.AppInsights.dll" target="lib\netstandard2.0" />
    <file src="bin\release\netstandard2.1\Skoda.Logging.AppInsights.dll" target="lib\netstandard2.1" />
    <file src="bin\release\net45\Skoda.Logging.AppInsights.dll" target="lib\net45" />
    <file src="bin\release\netstandard2.0\Skoda.Logging.AppInsights.xml" target="lib\netstandard2.0" />
    <file src="bin\release\netstandard2.1\Skoda.Logging.AppInsights.xml" target="lib\netstandard2.1" />
    <file src="bin\release\net45\Skoda.Logging.AppInsights.xml" target="lib\net45" />
    </files>
</package>